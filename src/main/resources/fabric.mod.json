{
  "schemaVersion": 1,
  "id": "ktdatataglib",
  "version": "${version}",
  "name": "KtDataTagLib",
  "description": "Kotlin library to manage data stored in the Minecraft Data API",
  "authors": [
    "NathanPB"
  ],
  "contact": {
    "homepage": "https://github.com/NathanPB/KtDataTagLib",
    "issues": "https://github.com/NathanPB/KtDataTagLib/issues",
    "sources": "https://github.com/NathanPB/KtDataTagLib.git"
  },
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.nathanpb.ktdatatag.example.ExampleModKt::init"
      }
    ]
  },
  "mixins": [],
  "depends": {
    "minecraft": "1.18.x",
    "fabric-language-kotlin": "*"
  },
  "custom": {
    "modmenu:api": true
  }
}
